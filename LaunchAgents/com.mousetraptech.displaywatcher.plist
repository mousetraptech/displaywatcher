<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.mousetraptech.displaywatcher</string>
  <key>ProgramArguments</key>
  <array>
    <string>/Users/Shared/displaywatcher/scripts/displaywatcher.sh</string>
  </array>
  <key>RunAtLoad</key>
  <true/>
  <key>KeepAlive</key>
  <true/>
  <key>StandardErrorPath</key>
  <string>/tmp/displaywatcher.err</string>
  <key>StandardOutPath</key>
  <string>/tmp/displaywatcher.out</string>
</dict>
</plist>